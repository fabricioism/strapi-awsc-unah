{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "Product",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "ProductID": {
      "type": "integer",
      "unique": true
    },
    "Name": {
      "type": "string"
    },
    "ProductNumber": {
      "type": "string",
      "required": false,
      "unique": true
    },
    "MakeFlag": {
      "type": "boolean"
    },
    "FinishedGoodsFlag": {
      "type": "boolean"
    },
    "Color": {
      "type": "string"
    },
    "SafetyStockLevel": {
      "type": "integer"
    },
    "ReorderPoint": {
      "type": "integer"
    },
    "StandardCost": {
      "type": "decimal"
    },
    "ListPrice": {
      "type": "decimal"
    },
    "Size": {
      "type": "string"
    },
    "SizeUnitMeasureCode": {
      "type": "string"
    },
    "WeightUnitMeasureCode": {
      "type": "string"
    },
    "Weight": {
      "type": "decimal"
    },
    "DaysToManufacture": {
      "type": "integer"
    },
    "ProductLine": {
      "type": "string"
    },
    "Class": {
      "type": "string"
    },
    "Style": {
      "type": "string"
    },
    "product_subcategory": {
      "model": "product-subcategory",
      "via": "products"
    },
    "SellStartDate": {
      "type": "datetime"
    },
    "SellEndDate": {
      "type": "datetime"
    },
    "DiscontinuedDate": {
      "type": "datetime"
    },
    "rowguid": {
      "type": "string"
    },
    "ModifiedDate": {
      "type": "datetime"
    },
    "product_model": {
      "model": "product-model",
      "via": "products"
    },
    "product_inventories": {
      "via": "product",
      "collection": "product-inventory"
    },
    "product_cost_histories": {
      "via": "product",
      "collection": "product-cost-history"
    },
    "transaction_histories": {
      "via": "product",
      "collection": "transaction-history"
    },
    "product_list_price_histories": {
      "via": "product",
      "collection": "product-list-price-history"
    },
    "product_reviews": {
      "via": "product",
      "collection": "product-review"
    },
    "bill_of_materials": {
      "via": "product_assembly_id",
      "collection": "bill-of-materials"
    }
