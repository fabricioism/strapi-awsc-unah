      "type": "integer",
      "required": true,
      "unique": true
    },
    "CurrencyRateDate": {
      "type": "datetime"
    },
    "AverageRate": {
      "type": "decimal"
    },
    "EndOfDayRate": {
      "type": "decimal"
    },
    "ModifiedDate": {
      "type": "datetime"
    },
    "FromCurrencyCode": {
      "via": "currency_rates",
      "model": "currency"
    },
    "ToCurrencyCode": {
      "via": "FromCurrencyCode",
      "model": "currency"
    }
  }
}
